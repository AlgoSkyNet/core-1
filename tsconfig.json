{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "target": "es6",
        "outDir": "./staging/core/",
        "rootDir": "./",
        "moduleResolution": "node"
    },
    "exclude": [
        "node_modules", "staging"
    ]
}
